<!ELEMENT biblioteca (libro*, autor*)>
        <!ELEMENT libro (titulo,fecha_publicacion, genero, editorial)>
        <!ELEMENT autor (nombre, genero_principal, nacionalidad, fecha_nacimiento)>
        <!ELEMENT titulo (#PCDATA)>
        <!ELEMENT fecha_publicacion (#PCDATA)>
        <!ELEMENT genero (#PCDATA)>
        <!ELEMENT editorial (#PCDATA)>
        <!ELEMENT nombre (#PCDATA)>
        <!ELEMENT genero_principal (#PCDATA)>
        <!ELEMENT nacionalidad (#PCDATA)>
        <!ELEMENT fecha_nacimiento (#PCDATA)>
        <!ELEMENT id (#PCDATA)>

        <!ATTLIST libro idref IDREF #REQUIRED>
        <!ATTLIST libro portada (jpg | png) #REQUIRED>
        <!ATTLIST autor foto (jpg | png) #REQUIRED>
        <!ATTLIST autor id ID #REQUIRED>
        <!ATTLIST autor pagina_web CDATA #IMPLIED>
        <!ATTLIST genero id ID #REQUIRED>
        <!ATTLIST genero_principal idref IDRED #REQUIRED>
        <!ATTLIST editorial URL #IMPLIED>
